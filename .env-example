# Configurações do banco de dados principal
DB_CONNECTION=mysql
DB_HOST=127.0.0.1
DB_PORT=3306
DB_DATABASE=pyquent
DB_USERNAME=root
DB_PASSWORD=
DB_CHARSET=utf8mb4
DB_COLLATION=utf8mb4_unicode_ci
DB_PREFIX=
DB_STRICT=true
DB_ENGINE=InnoDB

# Configurações do banco de dados secundário (PostgreSQL)
DB_SECONDARY_CONNECTION=pgsql
DB_SECONDARY_HOST=127.0.0.1
DB_SECONDARY_PORT=5432
DB_SECONDARY_DATABASE=pyquent_secondary
DB_SECONDARY_USERNAME=postgres
DB_SECONDARY_PASSWORD=
DB_SECONDARY_SCHEMA=public

# Configurações do banco de dados terciário (SQLite)
DB_TERTIARY_CONNECTION=sqlite
DB_TERTIARY_DATABASE=pyquent_tertiary.db

# Configurações de cache
CACHE_DRIVER=redis
CACHE_PREFIX=pyquent_
CACHE_TTL=3600

# Configurações de logging
LOG_QUERIES=true
LOG_SLOW_QUERIES=1000 