# Configurações da aplicação
APP_NAME=QuentORM
APP_URL=http://localhost
APP_KEY=sua_chave_aqui

# Configurações de banco de dados
DB_CONNECTION=sqlite
DB_HOST=localhost
DB_PORT=5432
DB_DATABASE=quentorm
DB_USERNAME=postgres
DB_PASSWORD=postgres

# Configurações de banco de dados secundário
DB_SECONDARY_CONNECTION=sqlite
DB_SECONDARY_HOST=localhost
DB_SECONDARY_PORT=5432
DB_SECONDARY_DATABASE=quentorm_secondary
DB_SECONDARY_USERNAME=postgres
DB_SECONDARY_PASSWORD=postgres

# Configurações de banco de dados terciário
DB_TERTIARY_CONNECTION=sqlite
DB_TERTIARY_HOST=localhost
DB_TERTIARY_PORT=5432
DB_TERTIARY_DATABASE=quentorm_tertiary.db
DB_TERTIARY_USERNAME=postgres
DB_TERTIARY_PASSWORD=postgres

# Configurações de cache
CACHE_DRIVER=file
CACHE_PREFIX=quentorm_
CACHE_TTL=3600

# Configurações de log
LOG_LEVEL=INFO
LOG_FILE=app.log

# Configurações de segurança
SECRET_KEY=sua_chave_secreta_aqui
SESSION_LIFETIME=120

# Configurações de email
MAIL_DRIVER=smtp
MAIL_HOST=smtp.gmail.com
MAIL_PORT=587
MAIL_USERNAME=
MAIL_PASSWORD=
MAIL_ENCRYPTION=tls
MAIL_FROM_ADDRESS=
MAIL_FROM_NAME=QuentORM

# Configurações do banco de dados principal
DB_CHARSET=utf8mb4
DB_COLLATION=utf8mb4_unicode_ci
DB_PREFIX=
DB_STRICT=true
DB_ENGINE=InnoDB

# Configurações do banco de dados secundário (PostgreSQL)
DB_SECONDARY_SCHEMA=public

# Configurações de logging
LOG_QUERIES=true
LOG_SLOW_QUERIES=1000 