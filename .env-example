# Configurações de ambiente
APP_ENV=development
APP_DEBUG=True
APP_NAME=QuentORM
APP_URL=http://localhost
APP_KEY=sua_chave_aqui
APP_LOCALE=pt_br

# Configurações de banco de dados
DB_CONNECTION=sqlite
DB_HOST=localhost
DB_PORT=5432
DB_DATABASE=quentorm
DB_USERNAME=postgres
DB_PASSWORD=postgres

# Configurações de cache
CACHE_DRIVER=file
CACHE_PREFIX=quentorm_
CACHE_TTL=3600

# Configurações de log
LOG_CHANNEL=file
LOG_LEVEL=INFO
LOG_FILE=app.log

# Configurações de segurança
SECRET_KEY=sua_chave_secreta_aqui
SESSION_LIFETIME=120

# Configurações de email
MAIL_DRIVER=smtp
MAIL_HOST=smtp.gmail.com
MAIL_PORT=587
MAIL_USERNAME=
MAIL_PASSWORD=
MAIL_ENCRYPTION=tls
MAIL_FROM_ADDRESS=
MAIL_FROM_NAME=QuentORM

